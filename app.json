{
  "name": "VENOM-XMD",
  "description": "A WhatsApp bot based on Baileysjs library (Ts+Js) with Multi Device support",
  "logo": "https://files.catbox.moe/lidsgj.jpg",
  "repository": "https://github.com/giftdee/VENOM-XMD",
  "success_url": "/",
  "keywords": ["nodejs", "bot", "whatsapp bot", "whatsapp automation", "multi device"],
  "stack": "container",
  "env": {
    "OWNER_NAME": {
      "description": "Enter your owner name here",
      "value": "Dave",
      "required": true
    },
    "API_KEY": {
      "description": "Enter your Heroku API key here (keep this secret)",
      "value": "",
      "required": true
    },
    "APP_NAME": {
      "description": "Your Heroku app name",
      "value": "VENOM-XMD",
      "required": true
    },
    "HEROKU_API": {
      "description": "Heroku API key",
      "value": "",
      "required": true
    },
    "BOT_PREFIX": {
      "description": "Bot command prefix",
      "value": ".",
      "required": false
    },
    "ANTIDELETE": {
      "description": "Enable anti-delete feature (true/false)",
      "value": "true",
      "required": false
    },
    "AUTO_TYPE": {
      "description": "Show fake typing in chats (true/false)",
      "value": "true",
      "required": false
    },
    "AUTO_RECORD": {
      "description": "Show fake recording in chats (true/false)",
      "value": "false",
      "required": false
    },
    "AUTO_STATUS": {
      "description": "Bot will view status automatically (true/false)",
      "value": "true",
      "required": false
    },
    "PACK_NAME": {
      "description": "Sticker pack name",
      "value": "VENOM-XMD pack",
      "required": false
    },
    "AUTHOR": {
      "description": "Sticker author name",
      "value": "Dave",
      "required": false
    },
    "SESSION_ID": {
      "description": "Your WhatsApp session ID",
      "value": "",
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "standard-2x"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql:essential-0"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}